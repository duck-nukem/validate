{
  "name": "typeValidator",
  "version": "1.0.0",
  "description": "Typescript Object validator",
  "main": "index.js",
  "author": "Alex Szab√≥",
  "license": "MIT",
  "dependencies": {
    "@types/jasmine": "^2.8.6",
    "codecov": "^3.0.0",
    "jest": "^22.4.2",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageReporters": [
      "json"
    ]
  }
}
